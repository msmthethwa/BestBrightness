@model BestBrightness.Models.Stock

<h2>Delivery Slip</h2>

<p><strong>Product Name:</strong> @Model.Name</p>
<p><strong>Quantity:</strong> @Model.Quantity</p>
<p><strong>Supplier:</strong> @Model.Supplier</p>
<p><strong>Received Date:</strong> @Model.ReceivedDate.ToString("yyyy-MM-dd")</p>
<p><strong>Product Image:</strong></p>
<img src="data:image/jpeg;base64,@Model.ImageBase64" alt="Product Image" style="max-width: 200px;" />
